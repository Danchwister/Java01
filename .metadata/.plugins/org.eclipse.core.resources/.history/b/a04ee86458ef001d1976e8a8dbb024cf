package curso_java;

public class Sum {

	public static void main(String[] args) {
		
		int a = 74;
		int b = 36;
		int somae = a +b ;
		int divideae =  a / b ;
		int multiplicae = a * b ;
		
		System.out.println(somae);
		System.out.println(divideae);
		System.out.println(multiplicae);
		
		

	}

}
